{
  "name": "jasper",
  "private": true,
  "version": "0.8.0-beta.4",
  "description": "A flexible and powerful issue reader for GitHub",
  "author": "h13i32maru",
  "homepage": "https://jasperapp.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/jasperapp/jasper"
  },
  "keywords": [],
  "main": "out/src/index.js",
  "scripts": {
    "mac:run": "npm run copy-icon && JASPER=DEV ./node_modules/.bin/electron ./",
    "mac:build": "./script/mac/build.sh",
    "mac:sqlite": "./script/mac/build-sqlite.sh",
    "win:run": "./node_modules/.bin/electron -r babel-register ./",
    "win:build": "./script/win/build.sh",
    "win:sqlite": "./script/win/build-sqlite.sh",
    "linux:run": "./node_modules/.bin/electron -r babel-register ./",
    "linux:build": "./script/linux/build.sh",
    "linux:sqlite": "./script/linux/build-sqlite.sh",
    "tsc:watch": "./script/tsc-watch.sh",
    "copy-icon": "cp ./misc/logo/jasper-dev.icns ./node_modules/electron/dist/Electron.app/Contents/Resources/electron.icns"
  },
  "dependencies": {
    "color-logger": "0.0.6",
    "diff": "3.5.0",
    "escape-html": "1.0.3",
    "fs-extra": "7.0.0",
    "moment": "2.22.2",
    "node-zip": "1.1.1",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "request": "2.88.0",
    "semver": "5.5.1",
    "sqlite3": "^4.1.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "electron": "6.0.7",
    "electron-notarize": "^0.1.1",
    "electron-packager": "14.0.4",
    "esdoc": "latest",
    "esdoc-jsx-plugin": "latest",
    "esdoc-standard-plugin": "latest",
    "typescript": "^3.5.3"
  }
}
